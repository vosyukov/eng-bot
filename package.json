{
  "name": "eng-bot",
  "version": "1.0.0",
  "description": "Telegram bot with inline buttons support",
  "main": "dist/index.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "ts-node --transpile-only src/index.ts",
    "dev:watch": "nodemon --exec ts-node --transpile-only src/index.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "drizzle:generate": "drizzle-kit generate:pg",
    "drizzle:migrate": "ts-node src/database/migrate.ts",
    "migrate:up": "drizzle-kit migrate",
    "migrate:down": "drizzle-kit rollback"
  },
  "dependencies": {
    "@google/genai": "^1.5.1",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/config": "^3.2.0",
    "@qdrant/js-client-rest": "^1.14.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.64.0",
    "openai": "^4.100.0",
    "openapi": "^1.0.1",
    "pg": "^8.16.0",
    "promptlayer": "^1.0.30",
    "reflect-metadata": "^0.2.1",
    "rss-parser": "^3.13.0",
    "rxjs": "^7.8.1",
    "telegraf": "^4.16.3",
    "zod": "^3.25.57"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@types/node-telegram-bot-api": "^0.64.2",
    "@types/pg": "^8.15.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.3.3"
  }
}
